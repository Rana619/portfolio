  <div id="div0" class="targetDiv">
    <div class="descriptionk">
      <span class="hide">X</span>
      <h1>All Messages</h1>
      <% allMessages.forEach(function(message){ %>
        <div class="mails">
          <details>
            <summary>
              <span style="font-size: 16px;">
                <%= message.mailId %>
              </span><br />
              <span style="font-size: 12px; position: relative; left: 15px; opacity: 0.5;">
                <%= message.subject %>
              </span>
              <span style="margin-left: 35px; opacity: 0.5; font-size: 12px;">
                <%= message.date %>
              </span>
            </summary>
            <div class="mailBody">
              <p style="opacity: 0.7; font-size: 12px; overflow: hidden;">
                <%= message.mailId %><span style="margin-left: 5px; margin-right: 5px;">>></span>
                  <%= message.subject %>
              </p>
              <p>
                <%= message.name %>,
              </p>
              <p>
                <%= message.message %>
              </p>
            </div>
          </details>
          <div class="delButton">
            <a href="/delete/<%= message._id %>">âœ—</a>
          </div>
        </div>
        <% }) %>
    </div>
  </div>



